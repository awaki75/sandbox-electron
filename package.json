{
  "name": "sandbox-electron",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "run-s test:prettier test:tslint test:tsc",
    "test:prettier": "prettier -l **/*.{json,js,jsx,ts,tsx}",
    "test:tslint": "tslint -p . src/**/*.{ts,tsx}",
    "test:tsc": "tsc --noEmit",
    "dev": "electron-webpack dev",
    "build": "electron-webpack && electron-builder"
  },
  "dependencies": {
    "source-map-support": "^0.5.6"
  },
  "devDependencies": {
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1"
  }
}

{
  "name": "sandbox-electron",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "run-p test:prettier test:tslint test:tsc test:jest",
    "test:prettier": "prettier -l **/*.{json,js,ts}",
    "test:tslint": "tslint -p . src/**/*.ts",
    "test:tsc": "tsc --noEmit",
    "test:jest": "jest",
    "coverage": "jest --coverage",
    "dev": "electron-webpack dev",
    "build": "electron-webpack && electron-builder"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "source-map-support": "^0.5.6",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "css-loader": "^1.0.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "electron-webpack-vue": "^2.1.3",
    "jest": "^23.3.0",
    "node-sass": "^4.9.2",
    "prettier": "^1.13.7",
    "sass-loader": "^7.0.3",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1"
  }
}

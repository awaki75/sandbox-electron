{
  "name": "sandbox-electron",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "run-p test:prettier test:tslint test:tsc test:jest",
    "test:prettier": "prettier -l **/*.{json,js,ts}",
    "test:tslint": "tslint -p . src/**/*.ts",
    "test:tsc": "tsc --noEmit",
    "test:jest": "jest",
    "coverage": "jest --coverage",
    "dev": "npm-run-all dev:main -p dev:renderer electron",
    "dev:main": "webpack --config build/webpack.main.config.js --mode development",
    "dev:renderer": "webpack-dev-server --config build/webpack.renderer.config.js --mode development",
    "build": "npm-run-all -p build:main build:renderer -s electron",
    "build:main": "webpack --config build/webpack.main.config.js --mode production",
    "build:renderer": "webpack --config build/webpack.renderer.config.js --mode production",
    "electron": "electron dist/main.js"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/webpack": "^4.4.7",
    "css-loader": "^1.0.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.3.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "sass-loader": "^7.0.3",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.4.2",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
